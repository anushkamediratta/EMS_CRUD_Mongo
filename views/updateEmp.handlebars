<div class="container">
    <h1>ALL  EMPLOYEE  DETAILS</h1>
    
    <table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">NAME</th>
      <th scope="col">Email</th>
      <th scope="col">CITY</th>
      <th scope="col">PHONE</th>
          </tr>
  </thead>

  <tbody>
    {{#each list}}
    <tr class="table-active">
      
       <td >{{this.fullName}}</td>
      <td >{{this.email}}</td>
      <td >{{this.city}}</td>
      <td >{{this.mobile}}</td>
    <td> <a href="/emp/pre-update/{{this._id}}" class="btn btn-info">Update</a></td>  
      
    </tr>
    {{/each}}
    
  </tbody>
</table>
</div>